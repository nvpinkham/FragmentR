## The function is currently defined as
function (x, peaks, m = 50, n = 50) 
{
    if (length(peaks) > 0) {
        peaks <- peaks + 51
        x <- c(rep(0, 50), x, rep(0, 50))
        area <- NULL
        for (j in 1:length(peaks)) {
            area[j] <- bayestestR::auc(1:101, x[(peaks[j] - m):(peaks[j] + 
                n)], "trapezoid")
        }
        return(area)
    }
  }
}
